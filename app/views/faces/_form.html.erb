<%= form_for @face do |f| %>
    <p>
      <%= f.label :name %><br/>
      <%= f.text_field :name %>
    </p>
    <p>
      <%= f.label :room %><br/>
      <%= f.text_field :room %>
    </p>
    <p>
      <%= f.label :modules %><br/>
      <%= f.text_field :modules %>
    </p>
    <p>
      <%= f.label :email %><br/>
      <%= f.text_field :email %>
    </p>
    <p>
      <%= f.label :link_to_photo %><br/>
      <%= f.text_field :photo %>
    </p>
    <p>
      <%= f.submit %>
    </p>
<% end %>